<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">

	<portlet>
		<portlet-name>agrar_dictionaries</portlet-name>
		<display-name>agrar_dictionaries</display-name>
		<portlet-class>agrar.dict.portlet.DictionariesPortlet</portlet-class>
		
		<init-param>
			<name>view-template</name>
			<value>/jsp/view.jsp</value>
		</init-param>
		
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>

		<supported-locale>ru</supported-locale>
		<resource-bundle>messages</resource-bundle>
		
		<portlet-info>
			<title>Районы</title>
			<short-title>Районы</short-title>
			<keywords>Районы</keywords>
		</portlet-info>
		
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
</portlet-app>